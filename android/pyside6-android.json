{
  "application": {
    "name": "Surfscape",
    "package": "org.surfscape.surfscape",
    "version": "1.0",
    "icon": "icon.png",
    "permissions": [
      "INTERNET",
      "ACCESS_NETWORK_STATE"
    ]
  },
  "qt": {
    "modules": [
      "core",
      "qml",
      "quick",
      "quickcontrols2",
      "quicklayouts",
      "quickdialogs2",
      "webview",
      "network",
      "gui",
      "opengl",
      "shadertools"
    ],
    "features": [
      "ssl",
      "http2"
    ]
  },
  "python": {
    "entrypoint": "main.py",
    "requirements": [
      "PySide6",
      "PySide6-Addons",
      "shiboken6"
    ]
  },
  "android": {
    "sdk": {
  "buildToolsVersion": "35.0.0",
  "compileSdkVersion": 35,
  "targetSdkVersion": 35,
  "minSdkVersion": 23,
  "ndkVersion": "27.1.12297006",
  "cmakeVersion": "3.30.5"
    },
    "architectures": [
      "arm64-v8a",
      "x86_64"
    ],
    "signing": {
      "keystorePath": "",
      "keystorePassword": "",
      "keyAlias": "",
      "keyPassword": ""
    }
  }
}
