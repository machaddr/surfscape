{
  "application": {
    "name": "Surfscape",
    "package": "org.surfscape.browser",
    "version": "1.0",
    "icon": "../icon/icon.png",
    "permissions": [
      "INTERNET"
    ]
  },
  "qt": {
    "modules": [
      "qml",
      "quick",
      "quickcontrols2",
      "quicklayouts",
      "quickdialogs2",
      "webview",
      "network"
    ],
    "features": []
  },
  "python": {
    "entrypoint": "main.py",
    "requirements": [
      "PySide6==6.6.3",
      "PySide6-Addons==6.6.3",
      "shiboken6==6.6.3"
    ]
  },
  "android": {
    "sdk": {
      "buildToolsVersion": "34.0.0",
      "compileSdkVersion": 34,
      "ndkVersion": "25.2.9519653",
      "cmakeVersion": "3.22.1"
    },
    "architectures": [
      "arm64-v8a"
    ],
    "signing": {
      "keystorePath": "",
      "keystorePassword": "",
      "keyAlias": "",
      "keyPassword": ""
    }
  }
}
