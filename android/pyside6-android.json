{
  "application": {
    "name": "Surfscape",
    "package": "org.surfscape.browser",
    "version": "1.0",
    "icon": "icon.png",
    "permissions": [
      "INTERNET"
    ]
  },
  "qt": {
    "modules": [
      "qml",
      "quick",
      "quickcontrols2",
      "quicklayouts",
      "quickdialogs2",
      "webview",
      "network"
    ],
    "features": []
  },
  "python": {
    "entrypoint": "main.py",
    "requirements": [
      "PySide6",
      "PySide6-Addons",
      "shiboken6"
    ]
  },
  "android": {
    "sdk": {
  "buildToolsVersion": "35.0.0",
  "compileSdkVersion": 35,
  "ndkVersion": "27.1.12297006",
  "cmakeVersion": "3.30.5"
    },
    "architectures": [
      "arm64-v8a"
    ],
    "signing": {
      "keystorePath": "",
      "keystorePassword": "",
      "keyAlias": "",
      "keyPassword": ""
    }
  }
